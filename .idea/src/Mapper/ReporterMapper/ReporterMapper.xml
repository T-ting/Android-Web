<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Mapper.ReporterMapper.ReporterMapper">

    <select id="Reporterlogin" parameterType="Reporter" resultType="Reporter">
        SELECT * FROM Reporter WHERE id = #{id} and password = #{password}
    </select>

</mapper>